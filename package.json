    "axios": "^0.21.1",
    "bulma": "^0.8.0",
    "cropperjs": "^1.5.6",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
